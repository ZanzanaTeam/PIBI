<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>{% block title %}Projet BI!{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    {% block stylesheets %}
        <!-- Bootstrap 3.3.7 -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/bootstrap/css/bootstrap.min.css') }}">
        <!-- Font Awesome -->
        <link rel="stylesheet"
              href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
        <!-- Theme style -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/dist/css/AdminLTE.min.css') }}">
        <!-- AdminLTE Skins. Choose a skin from the css/skins
             folder instead of downloading all of them to reduce the load. -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/dist/css/skins/_all-skins.min.css') }}">
        <!-- Morris chart -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/plugins/morris/morris.css') }}">
        <!-- jvectormap -->
        <link rel="stylesheet"
              href="{{ asset('bundles/espritprojetbi/plugins/jvectormap/jquery-jvectormap-1.2.2.css') }}">
        <!-- Date Picker -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/plugins/datepicker/datepicker3.css') }}">
        <!-- Daterange picker -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/plugins/daterangepicker/daterangepicker.css') }}">
        <!-- bootstrap wysihtml5 - text editor -->
        <link rel="stylesheet"
              href="{{ asset('bundles/espritprojetbi/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}">

        <!-- DataTables -->
        <link rel="stylesheet" href="{{ asset('bundles/espritprojetbi/plugins/datatables/dataTables.bootstrap.css') }}">
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

        <!-- Google Font -->
        <link rel="stylesheet"
              href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>

<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">


    {% block top_header %}
        <header class="main-header">
            <!-- Logo -->
            <a href="{{ path('esprit_projetbi_dashboard_index') }}" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>P</b>BI</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Projet</b>BI</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <span class="hidden-xs">
                                {{ app.user.username }}
                            </span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="{{ asset('bundles/espritprojetbi/dist/img/user2-160x160.jpg') }}"
                                         class="img-circle" alt="User Image">

                                    <p>
                                        {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}
                                    </p>
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-right">
                                        <a href="{{ path('fos_user_security_logout') }}"
                                           class="btn btn-default btn-flat">
                                            {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
    {% endblock top_header %}
    {% block menu_header %}
        {% include 'header_menu.html.twig' %}
    {% endblock menu_header %}

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        {% block content_header %}
        <section class="content-header">
            {% block page_title %}
            <h1>Tableau de bord<small></small>
            {% endblock page_title %}
            </h1>
            <ol class="breadcrumb">
                {% block breadcrumb %}
                <li><a href="{{ path('esprit_projetbi_dashboard_index') }}"><i class="fa fa-dashboard"></i> Home</a></li>
                {% endblock breadcrumb %}
            </ol>
        </section>
        {% endblock content_header %}

        <!-- Main content -->
        <section class="content">
            {% block content %}
            {% endblock content %}
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
    {% block footer %}
    <footer class="main-footer">
        <div class="pull-right hidden-xs">
            <b>Version</b> 1.0
        </div>
        <strong>Copyright &copy; 2017 <a href="https://github.com/ZanzanaTeam/">Zanzana Team</a>.</strong> All rights
        reserved.
    </footer>
    {% endblock footer %}
</div>
<!-- ./wrapper -->

</body>

{% block javascripts %}
    <!-- jQuery 3.1.1 -->
    <script src="{{ asset('bundles/espritprojetbi/plugins/jQuery/jquery-3.1.1.min.js') }}"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script src='{{ asset("bundles/espritprojetbi/bootstrap/js/bootstrap.min.js") }}'></script>
    <!-- Morris.js charts -->
    {#<script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>#}
    {#<script src="{{ asset('bundles/espritprojetbi/plugins/morris/morris.min.js') }}"></script>#}
    <!-- Sparkline -->
    {#<script src="{{ asset('bundles/espritprojetbi/plugins/sparkline/jquery.sparkline.min.js') }}"></script>#}
    {#<!-- jvectormap -->#}
    {#<script src="{{ asset('bundles/espritprojetbi/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>#}
    {#<script src="{{ asset('bundles/espritprojetbi/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>#}
    {#<!-- jQuery Knob Chart -->#}
    <script src="{{ asset('bundles/espritprojetbi/plugins/knob/jquery.knob.js') }}"></script>
    <!-- daterangepicker -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
    <script src="{{ asset('bundles/espritprojetbi/plugins/daterangepicker/daterangepicker.js') }}"></script>
    <!-- datepicker -->
    <script src="{{ asset('bundles/espritprojetbi/plugins/datepicker/bootstrap-datepicker.js') }}"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="{{ asset('bundles/espritprojetbi/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>
    <!-- Slimscroll -->
    <script src="{{ asset('bundles/espritprojetbi/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
    <!-- FastClick -->
    <script src="{{ asset('bundles/espritprojetbi/plugins/fastclick/fastclick.js') }}"></script>
    <!-- AdminLTE App -->
    <script src="{{ asset('bundles/espritprojetbi/dist/js/adminlte.min.js') }}"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    {#<script src="{{ asset('bundles/espritprojetbi/dist/js/pages/dashboard.js') }}"></script>#}
    <!-- AdminLTE for demo purposes -->
    {#<script src="{{ asset('bundles/espritprojetbi/dist/js/demo.js') }}"></script>#}

    <!-- DataTables -->
    <script src="{{ asset('bundles/espritprojetbi/plugins/datatables/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('bundles/espritprojetbi/plugins/datatables/dataTables.bootstrap.min.js') }}"></script>
{% endblock %}

</html>
